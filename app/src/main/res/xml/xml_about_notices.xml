<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Preference
        app:key="@string/key_credit_libraries"
        app:title="@string/open_source_libraries"/>

    <PreferenceCategory
        app:title="@string/credit_other_resources">

        <Preference
            app:key="@string/key_credit_notification_sound"
            app:title="@string/credit_notification_sound_title"
            app:summary="@string/credit_notification_sound_summary"/>

        <Preference
            app:key="@string/key_credit_launcher_icon"
            app:title="@string/credit_launcher_icon_title"
            app:summary="@string/credit_launcher_icon_summary"/>

        <Preference
            app:key="@string/key_credit_vector_illustrations"
            app:title="@string/credit_vector_illustrations_title"
            app:summary="@string/credit_vector_illustrations_summary"/>

        <Preference
            app:key="@string/key_credit_user_interface_icons"
            app:title="@string/credit_user_interface_icons_title"
            app:summary="@string/credit_user_interface_icons_summary"/>
    </PreferenceCategory>

</PreferenceScreen>